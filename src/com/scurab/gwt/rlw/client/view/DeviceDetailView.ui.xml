<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="com.scurab.gwt.rlw.language.Words" field="res" />
	<ui:style>
		.important {
			font-weight: bold;
		}
		
		.header {
			font-size: 14px;
			margin-bottom: 5px;
			font-weight: bold;
			min-width: 120px;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:CaptionPanel captionText="{res.Detail}">
			<g:Grid height="100%">
				<g:row>
					<g:customCell>
						<g:Label text="{res.ID}" styleName="{style.header}" />
					</g:customCell>
					<g:customCell>
						<g:Label ui:field="mID" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="{res.Brand}" styleName="{style.header}" />
					</g:customCell>
					<g:customCell>
						<g:Label ui:field="mBrand" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="{res.Description}" styleName="{style.header}" />
					</g:customCell>
					<g:customCell>
						<g:Label ui:field="mDescription" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="{res.Detail}" styleName="{style.header}" />
					</g:customCell>
					<g:customCell>
						<g:Label ui:field="mDetail" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="{res.DevUUID}" styleName="{style.header}" />
					</g:customCell>
					<g:customCell>
						<g:Label ui:field="mDevUUID" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="{res.Model}" styleName="{style.header}" />
					</g:customCell>
					<g:customCell>
						<g:Label ui:field="mModel" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="{res.OsDesription}" styleName="{style.header}" />
					</g:customCell>
					<g:customCell>
						<g:Label ui:field="mOSDescription" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="{res.Owner}" styleName="{style.header}" />
					</g:customCell>
					<g:customCell>
						<g:Label ui:field="mOwner" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="{res.Platform}" styleName="{style.header}" />
					</g:customCell>
					<g:customCell>
						<g:Label ui:field="mPlatform" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="{res.PushID}" styleName="{style.header}" />
					</g:customCell>
					<g:customCell>
						<g:Label ui:field="mPushID" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="{res.Resolution}" styleName="{style.header}" />
					</g:customCell>
					<g:customCell>
						<g:Label ui:field="mResolution" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="{res.Version}" styleName="{style.header}" />
					</g:customCell>
					<g:customCell>
						<g:Label ui:field="mVersion" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="{res.RawDetail}" styleName="{style.header}" />
					</g:customCell>
					<g:customCell>
						<g:Button ui:field="mRawDetailButton" text="{res.Open}" />
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:CaptionPanel>
	</g:HTMLPanel>
</ui:UiBinder> 