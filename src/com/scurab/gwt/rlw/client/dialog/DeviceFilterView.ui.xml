<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:p1="urn:import:com.google.gwt.user.datepicker.client">
	<ui:with type="com.scurab.gwt.rlw.language.Words" field="res"/>
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Grid width="550px" height="">
			<g:row>
				<g:customCell>
					<g:Label text="ID"/>
				</g:customCell>
				<g:customCell>
					<g:IntegerBox ui:field="mDeviceID" width="150px"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="{res.AppVersion}" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="mAppVersion" width="150px"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="UUID"/>
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="mUUID" width="97%"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="{res.Owner}"/>
				</g:customCell>
				<g:customCell>
					<g:TextBox width="97%" ui:field="mOwner"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="{res.Brand}"/>
				</g:customCell>
				<g:customCell>
					<g:ListBox visibleItemCount="1" ui:field="mBrand" width="100%"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="{res.Model}"/>
				</g:customCell>
				<g:customCell>
					<g:ListBox visibleItemCount="1" ui:field="mModel" width="100%"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="{res.Platform}"/>
				</g:customCell>
				<g:customCell>
					<g:ListBox visibleItemCount="1" ui:field="mPlatform" width="100%"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="{res.OSVersion}"/>
				</g:customCell>
				<g:customCell>
					<g:ListBox visibleItemCount="1" width="100%" ui:field="mOSVersion"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="{res.Resolution}"/>
				</g:customCell>
				<g:customCell>
					<g:ListBox visibleItemCount="1" ui:field="mResolution" width="100%"/>
				</g:customCell>
			</g:row>			
			<g:row>
				<g:customCell>
					<g:Label text="{res.Registered}" />
				</g:customCell>
				<g:customCell>
					<p1:DateBox width="96%" ui:field="mCreated"  />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="{res.Updated}" />
				</g:customCell>
				<g:customCell>
					<p1:DateBox width="96%" ui:field="mUpdated"  />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label/>
				</g:customCell>
				<g:customCell>
					<g:HorizontalPanel width="100%">
						<g:Button ui:field="mOkButton" text="{res.OK}" width="75px"/>
						<g:Button ui:field="mCancelButton" text="{res.Cancel}" width="75px"/>
					</g:HorizontalPanel>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 