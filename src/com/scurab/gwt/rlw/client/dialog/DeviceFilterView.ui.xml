<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="com.scurab.gwt.rlw.language.Words" field="res"/>
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Grid width="" height="">
			<g:row>
				<g:customCell>
					<g:Label text="ID"/>
				</g:customCell>
				<g:customCell>
					<g:IntegerBox ui:field="mDeviceID" width="150px"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="UUID"/>
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="mUUID" width="150px"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="{res.Brand}"/>
				</g:customCell>
				<g:customCell>
					<g:ListBox visibleItemCount="1" ui:field="mBrand" width="100%"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="{res.Model}"/>
				</g:customCell>
				<g:customCell>
					<g:ListBox visibleItemCount="1" ui:field="mModel" width="100%"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="{res.Platform}"/>
				</g:customCell>
				<g:customCell>
					<g:ListBox visibleItemCount="1" ui:field="mPlatform" width="100%"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="{res.Resolution}"/>
				</g:customCell>
				<g:customCell>
					<g:ListBox visibleItemCount="1" ui:field="mResolution" width="100%"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label/>
				</g:customCell>
				<g:customCell>
					<g:HorizontalPanel width="100%">
						<g:Button ui:field="mOkButton" text="{res.OK}" width="75px"/>
						<g:Button ui:field="mCancelButton" text="{res.Cancel}" width="75px"/>
					</g:HorizontalPanel>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 